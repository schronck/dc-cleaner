{
  "name": "cleaner",
  "version": "0.0.1",
  "description": "A Discord bot to keep your guild clean",
  "author": "ZGEN",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src/Main.ts",
    "build": "tsc",
    "prod": "node build/Main.js",
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint-fix": "eslint --fix . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --pretty --noEmit --incremental false"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/utilities": "^5.1.0",
    "discord.js": "^14.6.0",
    "discordx": "^11.4.0",
    "dotenv": "^16.0.3",
    "reflect-metadata": "^0.1.13",
    "simple-json-db": "^2.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "npm run lint-fix",
    "*.@(js|jsx|ts|tsx|css|md)": "npm run format"
  }
}
